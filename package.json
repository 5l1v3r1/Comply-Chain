{
    "name": "gov.dol.complychain",
    "version": "0.1.0",
    "private": true,
    "homepage": "./",
    "dependencies": {
        "cordova-android": "^7.1.4",
        "cordova-ios": "^4.5.5",
        "cordova-plugin-whitelist": "^1.3.3",
        "react": "^16.8.2",
        "react-dom": "^16.8.2",
        "react-router-dom": "^4.3.1",
        "react-scripts": "2.1.5"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "npm run remove:www && react-scripts build",
        "build:cordova": "npm run remove:www && react-scripts build && node _inject-cordova.js && mv build www",
        "build:cordova:win": "npm run remove:www:win && react-scripts build && node _inject-cordova.js && move build www",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "remove:www": "rm -rf www",
        "remove:www:win": "if exist www rd /s /q www"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": [
        ">0.2%",
        "not dead",
        "not ie <= 11",
        "not op_mini all"
    ],
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {}
        },
        "platforms": [
            "ios",
            "android"
        ]
    }
}